{
  "name": "super-styleguide",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "_____________________________Styles_____________________________": "",
    "stylelint": "stylelint 'src/scss/**/*.scss' || echo \"Stylelint failed for some file(s).\"",
    "clean:dev": "rimraf dist src/css/*.css",
    "scss:to:css:dev": "node-sass --output-style expanded src/scss/ -o src/css/",
    "css:to:postcss:dev": "postcss src/css --dir dist/css",
    "styles:dev": "npm run stylelint && npm run scss:to:css:dev && npm run css:to:postcss:dev",
    "_____________________________Dev_______________________________": "",
    "dev": "npm run clean:dev && npx uiengine build --serve --watch & npm run styles:dev"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@uiengine/adapter-ejs": "^2.5.9",
    "@uiengine/adapter-html": "^2.5.9",
    "@uiengine/core": "^2.5.9",
    "module-alias": "^2.2.2",
    "node-sass": "^4.13.0",
    "normalize-scss": "^7.0.1",
    "postcss": "^7.0.26",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "sass-mq": "^5.0.1",
    "stylelint": "^12.0.1",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.13.0"
  },
  "dependencies": {}
}
